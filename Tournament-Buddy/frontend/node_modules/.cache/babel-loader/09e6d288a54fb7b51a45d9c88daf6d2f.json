w>\r\n          </v-container>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"red lighten-2\" text @click=\"dialog = false; resetUser()\">Cancel</v-btn>\r\n          <v-btn color=\"#03DAC5\" :disabled=\"!isValid\" text @click=\"updateUser()\">Save</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n  </v-row>\r\n</template>\r\n\r\n<script>\r\nimport auth from '@/auth';\r\nexport default {\r\n    props:{\r\n        currentUser: Object\r\n    },\r\n  data() {\r\n    return {\r\n        user: {\r\n            firstName:'',\r\n            lastName:'',\r\n            email:''\r\n        },\r\n        userFirstNameRules: [\r\n          v => !!v || \"first name is required.\",\r\n        ],\r\n        userLastNameRules: [\r\n          v => !!v || \"last name is required.\",\r\n        ],\r\n        userEmailRules: [\r\n          v => !!v || \"E-mail is required.\",\r\n          v => /.+@.+/.test(v) || 'E-mail must be valid'\r\n        ],\r\n        isValid: true,\r\n        dialog: false\r\n    };\r\n  },\r\n  methods:{\r\n     resetUser(){\r\n         this.user = Object.assign({},this.currentUser);\r\n     },\r\n     updateUser(){\r\n       fetch(`${process.env.VUE_APP_REMOTE_API}/api/user`,{\r\n         method:'PUT',\r\n         headers:{\r\n            Authorization: 'Bearer ' + auth.getToken(),\r\n            Accept: \"application/json\",\r\n           'content-type': 'application/json'\r\n         },\r\n         body: JSON.stringify(this.user)\r\n       })\r\n       .then(response =>{\r\n         if(response.ok){\r\n           this.$emit('update-user');\r\n           this.dialog = false; \r\n         }\r\n       })\r\n     } \r\n  },\r\n  created(){\r\n      this.resetUser();\r\n  }\r\n  \r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"],"sourceRoot":"src/components"}]}