 class=\"shrink mr-2\"\r\n        contain\r\n        src=\"https://cdn.vuetifyjs.com/images/logos/vuetify-logo-dark.png\"\r\n        transition=\"scale-transition\"\r\n        width=\"40\"\r\n      />\r\n\r\n      <h1>Tournament Buddy</h1>\r\n    </div>\r\n\r\n    <v-spacer></v-spacer>\r\n\r\n    <router-link to=\"/browse\">\r\n      <v-btn target=\"_blank\" text v-if=\"user === null\">Browse</v-btn>\r\n    </router-link>\r\n    <router-link to=\"/login\">\r\n      <v-btn target=\"_blank\" text v-if=\"user === null\">Login</v-btn>\r\n    </router-link>\r\n    <router-link to=\"/register\">\r\n      <v-btn target=\"_blank\" text v-if=\"user === null\">Register</v-btn>\r\n    </router-link>\r\n\r\n    <v-menu offset-y v-if=\"user !== null\">\r\n      <template v-slot:activator=\"{ on }\">\r\n        <v-btn target=\"_blank\" text v-on=\"on\">Teams</v-btn>\r\n      </template>\r\n      <v-list>\r\n        <v-list-item>\r\n          <h4 @click=\"$router.push('/browse-teams')\" class=\"clickable\">Join Team</h4>\r\n        </v-list-item>\r\n        <v-list-item>\r\n          <h4 @click=\"$router.push('/create-team')\" class=\"clickable\">Create Team</h4>\r\n        </v-list-item>\r\n        <v-divider></v-divider>\r\n        <v-list-item v-for=\"(item, index) in teams\" :key=\"index\">\r\n          <v-list-item-title\r\n            class=\"clickable\"\r\n            @click=\"$router.push('/team-page/' + item.teamId)\"\r\n          >{{ item.teamName }}</v-list-item-title>\r\n        </v-list-item>\r\n      </v-list>\r\n    </v-menu>\r\n    <v-menu offset-y v-if=\"user !== null\">\r\n      <template v-slot:activator=\"{ on }\">\r\n        <v-btn target=\"_blank\" text v-on=\"on\">Tournaments</v-btn>\r\n      </template>\r\n      <v-list>\r\n        <v-list-item>\r\n          <h4 class=\"clickable\" @click=\"$router.push('/browse-tournaments')\">Join Tournament</h4>\r\n        </v-list-item>\r\n        <v-list-item>\r\n          <h4 class=\"clickable\" @click=\"$router.push(